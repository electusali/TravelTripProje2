@using TravelTripProje2.Models.Siniflar
@model List<Hakkımızda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TextLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            
            @foreach (var item in Model)
            {
                <img src="@item.FotoUrl" alt="" />
                <p>@item.Aciklama</p>
            }

        </div> 
    </div>
</div>


